# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'asd' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for asd
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Database'
  pod 'Firebase/Core'
  pod 'Alamofire','~>4.4'
  pod 'AlamofireImage','~>3.2'
  pod 'Kanna','~>5.0.0'

  target 'asdTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Analytics'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Database'
    pod 'Firebase/Core'
    pod 'Alamofire','~>4.4'
    pod 'AlamofireImage','~>3.2'
    pod 'Kanna','~>5.0.0'
  end

  target 'asdUITests' do
    # Pods for testing
    pod 'Firebase/Analytics'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Database'
    pod 'Firebase/Core'
    pod 'Alamofire','~>4.4'
    pod 'AlamofireImage','~>3.2'
    pod 'Kanna','~>5.0.0'
  end

end
